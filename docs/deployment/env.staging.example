# üß™ Ambiente de Staging - Roteirar IA v2.1.3
# Este arquivo serve como exemplo para configurar o ambiente de staging

# Configura√ß√£o do ambiente
VITE_ENVIRONMENT=staging
VITE_APP_VERSION=2.1.3
VITE_BUILD_TIME=auto_generated
VITE_GIT_COMMIT=auto_generated
VITE_GIT_BRANCH=auto_generated

# Firebase Staging
# Substitua pelos valores do seu projeto Firebase de staging
VITE_FIREBASE_API_KEY=your_staging_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=roteirar-staging.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=roteirar-staging
VITE_FIREBASE_STORAGE_BUCKET=roteirar-staging.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_staging_messaging_sender_id
VITE_FIREBASE_APP_ID=your_staging_firebase_app_id

# APIs de Terceiros - Staging
# Use chaves de desenvolvimento/staging
VITE_GEMINI_API_KEY=your_staging_gemini_api_key

# Analytics - Staging
# Use um measurement ID separado para staging
VITE_ANALYTICS_MEASUREMENT_ID=your_staging_ga4_measurement_id
VITE_ANALYTICS_DEBUG=true

# Configura√ß√µes de Debug (habilitadas em staging)
VITE_ENABLE_LOGGING=true
VITE_SHOW_DEBUG_INFO=true
VITE_ENABLE_ERROR_OVERLAY=true
VITE_ENABLE_DEVTOOLS=true

# URLs de API (se aplic√°vel)
VITE_API_URL=https://staging-api.roteirar.ai
VITE_WEBSOCKET_URL=wss://staging-ws.roteirar.ai

# Feature flags para staging
VITE_FEATURE_VOICE_SYNTHESIS=true
VITE_FEATURE_COLLABORATION=true
VITE_FEATURE_ANALYTICS=true
VITE_FEATURE_TEMPLATES=true
VITE_FEATURE_PWA=true

# Configura√ß√µes espec√≠ficas de staging
VITE_ALLOW_TEST_USERS=true
VITE_SKIP_ANALYTICS=false
VITE_MOCK_EXTERNAL_APIS=false 