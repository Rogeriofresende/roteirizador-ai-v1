{
  "name": "roteirizador-app",
  "private": true,
  "version": "2.1.3",
  "description": "Aplicação para geração de roteiros de vídeo com IA",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "dev:clean": "npm run clean && vite",
    "build": "tsc && vite build",
    "build:staging": "NODE_ENV=staging tsc && vite build",
    "build:production": "NODE_ENV=production tsc && vite build",
    "lint": "eslint src/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "preview": "vite preview",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "jest --config jest.config.optimized.cjs --coverage",
    "test:watch": "jest --watch",
    "clean": "rm -rf node_modules_old* .archive coverage playwright-report test-results dist build",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean:all && npm install",
    "deploy:staging": "./scripts/deploy-staging.sh",
    "deploy:green": "./scripts/deploy-green.sh",
    "deploy:switch": "./scripts/switch-production.sh",
    "deploy:rollback": "./scripts/rollback.sh",
    "lighthouse": "lighthouse http://localhost:4173 --output html --output-path ./lighthouse-report.html",
    "lighthouse:staging": "lighthouse https://staging.roteirar.ai --output html --output-path ./lighthouse-staging.html",
    "lighthouse:production": "lighthouse https://app.roteirar.ai --output html --output-path ./lighthouse-production.html",
    "health:check": "curl -f http://localhost:4173/health || exit 1",
    "security:audit": "npm audit && npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "performance:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "quality:check": "./scripts/quality-gates-check.sh",
    "quality:gates": "npm run build && npm run quality:check",
    "monitor:performance": "node performance-monitor.js",
    "build:monitor": "npm run build && npm run monitor:performance",
    "test:fast": "jest --config jest.config.optimized.cjs --maxWorkers=50%",
    "test:silent": "jest --config jest.config.optimized.cjs --silent",
    "learning:scan": "node scripts/learning-recovery-cli.js scan",
    "learning:report": "node scripts/learning-recovery-cli.js report",
    "learning:apply": "node scripts/learning-recovery-cli.js apply",
    "learning:pending": "node scripts/extended-learning-recovery-cli.js analyze",
    "learning:status": "node scripts/extended-learning-recovery-cli.js status",
    "learning:roadmap": "node scripts/extended-learning-recovery-cli.js plan",
    "monitor:start": "node scripts/error-monitor.js",
    "monitor:analyze": "node scripts/error-analyzer.js",
    "monitor:status": "cat logs/error-analysis.json | jq '.totalErrors' 2>/dev/null || echo '0'",
    "auto-fix": "node scripts/auto-fix-orchestrator.js",
    "auto-fix:generate": "node scripts/prompt-generator.js",
    "auto-fix:status": "cat logs/auto-fix-report.json | jq '.status' 2>/dev/null || echo 'Not executed'",
    "system:health": "npm run build && npm run monitor:analyze"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "firebase": "^10.13.2",
    "framer-motion": "^11.11.17",
    "lucide-react": "^0.523.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.26.2",
    "recharts": "^3.0.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^4.2.4",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@playwright/test": "^1.48.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "globals": "^15.11.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vite-bundle-analyzer": "^0.23.0",
    "vite-plugin-pwa": "^0.20.5"
  },
  "keywords": [
    "ia",
    "roteiro",
    "video",
    "content-creation",
    "react",
    "typescript"
  ],
  "author": "Rogerio Resende",
  "license": "MIT"
}
